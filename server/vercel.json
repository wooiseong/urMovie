{
  "version": 2,
  "installCommand": "cd .. && npm install",
  "buildCommand": "cd ../packages/types && npm run build && cd ../../server",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ]
}
