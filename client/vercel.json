{
  "version": 2,
  "installCommand": "cd .. && npm install",
  "buildCommand": "cd ../packages/i18n && npm run build && cd ../../client && npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
