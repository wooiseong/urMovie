{
  "version": 2,
  "installCommand": "cd .. && npm install",
  "buildCommand": "cd .. && npm run build --workspace=@urmovie/i18n && npm run build --workspace=client",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
